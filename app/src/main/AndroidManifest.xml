<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evandisoft.multistagedeltav2.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/my_launcher2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/my_launcher2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.evandisoft.multistagedeltav2.app.MainActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.evandisoft.multistagedeltav2.app.EditStage"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>